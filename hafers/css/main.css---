/*Header NavBar Style Start here*/

.header-nav {
    position: relative;
    display: inline-block;
    width: 100%;
    background-color: #fff;
    z-index: 999999;
}

.header-nav::before {
    background: rgba(0, 0, 0, 0) url("../images/line-img.jpg") no-repeat scroll center top / cover;
    content: "";
    height: 8px;
    position: absolute;
    bottom: 0;
    width: 100%;
}

.header-nav::after {
    background: rgba(0, 0, 0, 0) url("../images/line-img.jpg") no-repeat scroll center top / cover;
    content: "";
    height: 8px;
    position: absolute;
    top: 0;
    width: 100%;
}

.header-nav .navbar {
    margin: 0px;
    z-index: 999999;
}

.header-nav .navbar ul li {
    list-style-type: none;
    text-transform: uppercase;
}

.header-nav .navbar ul li a {
    color: #28486d;
    font-weight: bold;
    font-size: 24px;
    margin: 0;
    padding: 25px 16px 25px 42px;
    text-decoration: none;
    display: inline-block;
}

.header-nav .navbar ul li a:hover {
    background: url(../images/nanbar-img.png) no-repeat left;
    color: #b60101;
}

.header-nav .social-icon {
    display: inline-block;
