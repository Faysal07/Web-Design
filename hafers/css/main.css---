/*
@import url('https://fonts.googleapis.com/css?family=Roboto:100,100i,300,300i,400,400i,500,500i,700,700i,900,900i');
@import url('https://fonts.googleapis.com/css?family=Roboto+Condensed:300,300i,400,400i,700,700i');
*/

body {
    font-family: 'Roboto', sans-serif;
    font-size: 18px;
    color: #000;
    line-height: 20px;
    font-weight: normal;
    background: #eee;
    margin: 0px;
/*
    background: url(../images/backgroundimg.png) no-repeat;
    background-size: cover;
*/
}

* {
    margin: 0px;
    padding: 0px;
}

.clr {
    clear: both;
}

.master {
    width: 100%;
}

.header-grp {
    background: #fff;
    padding-top: 3px;
    -webkit-transition: all 0.6s ease;
    -moz-transition: all 0.6s ease;
    -o-transition: all 0.6s ease;
    -ms-transition: all 0.6s ease;
    transition: all 0.6s ease;
    height: 93px;
    position: absolute;
    width: 100%;
    top: 0px;
    z-index: 9999999999;
}

/*goToTop*/

.goToTop {
    position: fixed;
    width: 100%;
    z-index: 9999999999;
    top: 0px;
    height: 80px;
}

.goToTop .logo img {
    max-width: 210px;
    height: auto;
}

.goToTop .hdr-nav {
    margin-top: -3px;
}

.goToTop .hdr-contact {
    margin-top: 12px;
}

.header {
    width: 999px;
    height: 89px;
    display: block;
    display: block;
    margin: 0 auto;
}



.logo {
    float: left;
    width: 294px;
    margin-right: 60px;
    padding: 10px 0;
    margin-left: 16px;

}

.logo img {
    max-width: 100%;
    -webkit-transition: all 0.6s ease;
    -moz-transition: all 0.6s ease;
    -o-transition: all 0.6s ease;
    -ms-transition: all 0.6s ease;
    transition: all 0.6s ease;
}

.hdr-nav {
    float: left;
    width: 322px;
    -webkit-transition: all 0.6s ease;
    -moz-transition: all 0.6s ease;
    -o-transition: all 0.6s ease;
    -ms-transition: all 0.6s ease;
    transition: all 0.6s ease;
}

.hdr-nav ul {
    margin: 0;
    padding: 0;
    line-height: none;
}


.hdr-nav ul li {
    display: inline-block;
    float: none;
    margin: 30px 11px;
}

.hdr-nav ul li a {
    color: #000000;
    font-size: 15px;
    text-decoration: none;
    text-align: center;
    font-weight: bold;
    padding: 5px;
    border-bottom: 2px #fff solid;
}


.hdr-nav ul li a:hover {
    color: #74a500;
    border-bottom: 2px #74a500 solid;
}

.hdr-nav ul li a.active {
    color: #74a500;
    border-bottom: 2px #74a500 solid;
}

.hdr-contact {
    width: 217px;
    float: right;
    margin: 20px 16px 20px 0;
    -webkit-transition: all 0.6s ease;
    -moz-transition: all 0.6s ease;
    -o-transition: all 0.6s ease;
    -ms-transition: all 0.6s ease;
    transition: all 0.6s ease;
}

.hdr-contact ul {
    margin: 0px;
    padding: 0px;
    list-style: none;
}

.hdr-contact ul li {
    text-align: right;
    font-size: 20px;
    font-weight: 600;
    color: #000;
    letter-spacing: .41px;
    padding-bottom: 5px;
}

.hdr-contact ul li.ph-icon {
    background: url(../images/ph-number.png) no-repeat 0 2px;
    letter-spacing: 0px;
}

.hdr-contact ul li .hdr-email {
    font-size: 17px;
    font-weight: 400;
    color: #000;
    letter-spacing: .75px;
    text-decoration: none;
}


.hdr-contact ul li .hdr-email span img {
    width: 19px;
    height: 18px;
    margin-right: 10px;
    margin-bottom: -5px;
}

/*SLIDER GRP*/

.slider-grp {
    width: 100%;
    height: 480px;
    margin: 0 auto;
    background: url(../images/sliderbg.jpg) no-repeat top center;
    border-bottom: 1px #FFF solid;
    float: left;
    margin-top: 93px;
    position: relative;
}


.slider-wrap {
    width: 999px;
    margin: 0 auto;
    position: relative;
    height: 100%;
}

.slider-box {
    position: absolute;
    background: #fff;
    border-radius: 5px;
    width: 450px;
    top: 50px;
    right: 16px;
    padding: 5px;
}

.bx-wrapper {
    margin-bottom: 0px;
}

.bxslider {
    z-index: -9999;
}

.bxslider li img {
    width: 100%;
    display: block;
    background-size: cover;
}

.bx-wrapper .bx-viewport {
    background: transparent;
    left: 0;
    box-shadow: none;
    border: 0px;
}


.bx-wrapper .bx-pager.bx-default-pager a {
    background: #D3D700;
    height: 10px;
    width: 10px;
    border-radius: 100%;
}

.bx-wrapper .bx-pager.bx-default-pager a:hover,
.bx-wrapper .bx-pager.bx-default-pager a.active {
    background: #7FAF00;
}


.bx-wrapper .bx-prev {
    right: 46px !important;
    left: inherit;
}

.bx-wrapper .bx-prev {
    background: url(../images/slider-control-left.png) no-repeat;
}


.bx-wrapper .bx-next {
    background: url(../images/slider-control-right.png) no-repeat;
}

.bx-wrapper .bx-next:hover {
    background-position: inherit;

}

.bx-wrapper .bx-controls-direction a {
    height: 33px;
    width: 33px;
}

.bx-wrapper .bx-controls-direction a {
    bottom: 13%;
    top: inherit;
}

.moreview {
    font-size: 15px;
    color: #7db300;
    position: absolute;
    z-index: 99;
    width: 85px;
    height: 22px;
    background: #000;
    text-align: center;
    padding: 3px 0;
    border: 2px #7CB100 solid;
    border-radius: 5px;
    right: 90px;
    bottom: 33px;
    font-size: 15px;
    text-decoration: none;
}

.slider-ribbon {
    position: absolute;
    background: url(../images/slider-ribbon.png) no-repeat left;
    height: 33px;
    width: 129px;
    font-size: 19px;
    color: #000000;
    padding: 5px 12px;
    bottom: 30px;
    left: 5px;
    line-height: 30px;
    z-index: 999999;
}

.blank {
    height: 125px;
}

.spc-increase {
    margin-bottom: 45px !important;
}

.serarce-box {
    position: absolute;
    bottom: -1px;
    left: 16px;
    background: url(../images/sbg.jpg) repeat-x top center;
    width: 393px;
    height: 41px;
    border: solid 1px #fff;
    border-bottom: 0px;
    -webkit-border-top-left-radius: 5px;
    -webkit-border-top-right-radius: 5px;
    -moz-border-radius-topleft: 5px;
    -moz-border-radius-topright: 5px;
    border-top-left-radius: 5px;
    border-top-right-radius: 5px;
}

.sicon {
    float: left;
    margin: 12px 0 0 12px;
}

.search {
    background: #666;
    border-top: 1px #fff solid;
    border-right: 1px #fff solid;
    border-left: 1px #fff solid;
    border-bottom: none;
    padding: 12px;
}

.sbox {
    float: left;
    margin: 5px 0 0 10px;
    position: relative;
}

.sbox,
.sbox input[type="text"] {
    background: transparent;
    border: 0px;
    padding: 7px 7px;
    font-weight: normal;
    width: 290px;
    font-size: 19px;
    color: #d2d800;
    font-family: 'Conv_ProximaNova-Regular';
    text-shadow: 0 0 2px rgba(0, 0, 0, .75);
}

.sbox input[type="submit"] {
    font-size: 0px;
    background: url(../images/src-bttn.png) no-repeat top center;
    border: 0px;
    padding: 19px 15px;
    cursor: pointer;
}

/*FOOTER GRP*/

.footer-top {
    background: #EEEEEE;
    width: 100%;
    margin: 10px 0;
}

.footer-wrap {
    width: 999px;
    display: block;
    margin: 0 auto;
}

.ftr-company {
    margin-left: 17px;
    width: 146px;
    float: left;
    margin-right: 73px;
    min-height: 205px;
    background: url(../images/ftr-step.png) no-repeat right top 30px;
}

.ftr-company h3 {
    font-size: 16px;
    font-weight: 600;
    color: #9c9c9c;
    margin-top: 20px;
    margin-bottom: 5px;
}

.ftr-company ul {
    margin: :0px;
    padding: 0px;
    list-style: none;
}

.ftr-company ul li {
    margin: 1px 0;
}

.ftr-company ul li a {
    font-size: 14px;
    font-weight: 400;
    color: #9b9b9b;
    text-decoration: none;
}

.ftr-contact {
    width: 370px;
    margin-right: 80px;
}

.ftr-contact ul li {
    font-size: 14px;
    font-weight: 400;
    color: #9b9b9b;
    text-decoration: none;
    padding-left: 37px;
    line-height: 26px;
    margin: 0px;
    line-height: 32px;
}

.ftr-contact ul li span {
    font-weight: 600;

}

.pnh {
    background: url(../images/phn.jpg) no-repeat left top;
}

.con-email {
    background: url(../images/ftr-email.jpg) no-repeat left top 3px;
    padding-bottom: 4px;
}

.place {
    background: url(../images/place.jpg) no-repeat left top;
    line-height: 17px !important;
}

.ftr-social {
    float: right;
    display: block;
    width: 280px;
    margin-top: 20px;
}

.ftr-social-icon {
    margin-bottom: 20px;
    display: block;
}

.ftr-social h5 {
    font-size: 15px;
    font-weight: 600;
    font-style: italic;
    color: #979797;
    float: left;
    margin-right: 7px;
    line-height: 35px;
}

.ftr-social ul {
    margin: 0px;
    padding: 0px;
    list-style: none;
    line-height: normal;
}

.ftr-social ul li {
    margin: 5px;
    float: left;
}

.ftr-social ul li a {
    text-decoration: none;
}

.ftr-social .ftr-logo {
    margin-top: 33px;
    float: left;
    margin-right: 16px;
}

.ftr-btm {
    background: #FFFFFF;
}

.ftr-btm-wrp {
    width: 999px;
    margin: 0 auto;
    display: block;
}

.ftr-btm p {
    padding: 10px 0 10px 0;
    font-size: 12px;
    color: #9b9b9b;
    text-align: center;
}

.service-grp {
    background: #1F2128;
    padding: 63px 0 58px 0;
}

.service-wrap {
    width: 999px;
    margin: 0 auto;
    text-align: center;
}

.title {
    font-family: 'Conv_ProximaNova-Regular';
    font-size: 25px;
    color: #d2d800;
    font-weight: lighter;
    line-height: 40px;
    text-align: center;
    background: url(../images/svc-line.jpg) no-repeat bottom center;
}

.service-wrap ul {
    margin: 0px;
    padding: 0px;
    list-style: none;
}

.service-wrap ul li {
    width: 185px;
    display: inline-block;
    margin: 53px 20px 15px 20px;
}

.service-wrap ul li img {
    display: block;
    margin: 0 auto;
    padding-bottom: 20px;
}

.service-wrap h5 {
    font-family: 'Conv_ProximaNova-Regular';
    font-size: 30px;
    color: #fff;
    text-align: center;
    padding-bottom: 10px;
    font-weight: lighter;
    padding-top: 10px;
}

.service-wrap p {
    font-family: 'Conv_ProximaNova-Regular';
    font-size: 21px;
    color: #fff;
    text-align: center;
    font-weight: lighter;

}

.chooseus-grp {
    background: url(../images/choosebg.jpg) no-repeat top center;
    width: 100%;
}

.choose-wrp {
    width: 999px;
    margin: 0 auto;
    padding-top: 30px;
    text-align: justify;
}

.choose-ttl {
    color: #fff;
    background: url(../images/choose-line.jpg) no-repeat bottom center;
}

.choose-wrp p {
    margin-top: 30px;
    color: #000000;
    line-height: 21px;
    font-size: 17px;
    padding: 0px 30px 178px 32px;
}

.content-box {
    color: #9b9b9b;
    margin-left: 16px;
    padding-top: 15px;
    font-size: 14px;
}

.content-box h3 {
    margin-left: 0px !important;
    margin-bottom: 15px;
}

ul.con-info {
    float: left;
    margin-left: 0px !important;
    width: 450px;
}

ul.con-info li {
    color: #9b9b9b;
    font-size: 14px;
    font-weight: 400;
    line-height: 25px;
    margin: 0;
    padding-left: 37px !important;
    width: 100%;
    text-decoration: none;
    list-style: none;
    margin-bottom: 12px;
}

ul.con-info li.place {
    background: url("../images/place-con.png") no-repeat 0px 7px;
    line-height: 20px !important;
}

ul.con-info li.pnh {
    background: url("../images/phone-con.png") no-repeat 0px 3px;
}

ul.con-info li.con-email {
    background: url("../images/email-con.png") no-repeat 0px 5px;
}

.contact-form {
    float: right;
    width: 400px;
}

.contact-form .form-control {
    width: 360px;
    height: 25px;
    display: block;
    margin: 10px 0;
    padding: 8px;
    border-radius: 5px;
    background: #fff;
    border: 1px #A7A7A7 solid;
    -moz-box-shadow: 3px 3px 4px -2px rgba(0, 0, 0, 0.50) inset;
    -webkit-box-shadow: 3px 3px 4px -2px rgba(0, 0, 0, 0.50) inset;
    box-shadow: 3px 3px 4px -2px rgba(0, 0, 0, 0.50) inset;
}

.contact-form textarea {
    height: 100px !important;
}

.contact-form input[type="submit"] {
    display: inline;
    cursor: pointer;
}

.contact-form h3 {
    padding-left: 35px;
    background: url(../images/email-con-large.png) no-repeat 0 4px;
}

/*property*/

.property {
    width: 100%;
    padding: 40px 0 60px 0;
    background: url(../images/propertry-bg.jpg) repeat top center;

}

.rproperty {
    background: url(../images/rpropertry-bg.jpg) repeat top center !important;
    width: 100%;
    padding: 40px 0 50px 0;

}

.property-wrp {
    width: 999px;
    margin: 0 auto;
    text-align: center;
}

.property-wrp h3 {
    font-size: 25px;
    color: #d3d900;
    font-weight: lighter;
    line-height: 50px;
    margin-top: 40px;
    font-family: 'Conv_ProximaNova-Regular';
    background: url(../images/property-line.jpg) no-repeat bottom center;
}

.property-wrp h6 {
    font-size: 12px;
    color: #7faf00;
    font-weight: lighter;
    font-style: italic;
    font-family: 'Conv_ProximaNova-Regular';
    margin-bottom: 27px;
}

.pro-box {
    margin: 0px;
    padding: 0px;
    list-style: none;
    width: auto;
}

.pro-box li {
    display: inline-block;
    width: 330px;
    position: relative;
    overflow: hidden;
    cursor: pointer;

}

.hover-effect {
    /*	background:#CF3;*/
    height: 190px;
    width: 330px;
    position: absolute;
    top: -330px;
    left: 0;
    border-radius: 5px;
    background-color: #d3d900;
    -webkit-transition: all 0.6s ease;
    -moz-transition: all 0.6s ease;
    -o-transition: all 0.6s ease;
    -ms-transition: all 0.6s ease;
    transition: all 0.6s ease;
}

.pro-box li a .hover-effect {
    top: 0px;
}

.hover-effect h4 {
    font-size: 15px;
    color: #000;
    font-weight: 300;
    font-family: 'Conv_ProximaNova-Regular', 'Open Sans';
    padding: 16px 12px 16px 12px;

}

.pro-box h4 .date-pro {
    font-weight: bold;

}

.pro-det {
    margin: 0px;
    padding: 0px;
    list-style: none;
}

.pro-det li {
    display: inline-block;
    margin: 0 auto;
    width: 90px;
    box-shedow: 0px;
    float: none;
}

.pro-det li img {
    text-align: center;
    display: block;
    margin: 0 auto;
}

.pro-det li p {
    font-size: 13px;
    font-weight: 600;
}

.viewdetails {
    display: block;
    width: 118px !important;
    height: 24px !important !;
    background: #000;
    color: #d2d700;
    border: none;
    font-size: 15px;
    margin-top: 7px;
    margin: 7px 80px 7px 80px;
    text-align: center;
    padding: 2px 0;
    margin: 0 auto;
    text-decoration: none;
    margin-top: 10px;
}

.pro-box li:hover .hover-effect {
    top: 0;
    cursor: pointer;
    display: block;
}

.property-cap {
    background-color: rgba(0, 0, 0, 0.6);
    position: absolute;
    z-index: 1;
    bottom: 0;
    left: 0;
    right: 0;
    height: 45px;
    widh: 330px;
    margin: 10px 0;
    -webkit-transition: all 0.6s ease;
    -moz-transition: all 0.6s ease;
    -o-transition: all 0.6s ease;
    -ms-transition: all 0.6s ease;
    transition: all 0.6s ease;
}

.property-cap h5 {
    background: url(../images/recomand-ribbon.png) no-repeat left;
    text-align: left;
    padding: 12px 20px;
    font-size: 19px;
    color: #000;
    font-weight: 400;
    display: inline-block;
    float: left;
    padding-right: 30px;
}

.property-cap p {
    font-size: 14px;
    font-weight: lighter;
    color: #fff;
    display: inline-block;
    float: left;
    margin-left: 20px;
    line-height: 17px;
    padding-top: 5px;
}

.property-cap p span {
    font-size: 14px;
    font-weight: lighter;
    color: #fff;
    display: block;
    font-weight: bold;
    color: #d2d700;
}

.recomand-hover {
    background-color: rgba(0, 0, 0, 0.7) !important;
}

.pro-box li:hover .property-cap {
    bottom: -60px;
}

.recomand-hover h4 {
    color: #fff;
}

.recomand-hover h4 .date-pro {
    color: #d2d700;
}

.recomand-hover .viewdetails {
    border: 1px #D3D900 solid;
    border-radius: 3px;
}

.recomand-hover .pro-det li p {
    color: #fff;
}

.viewmore {
    color: #494d5a;
    text-align: center;
    font-size: 17px;
    font-weight: bold;
    text-decoration: underline;
    text-transform: uppercase;
    padding-top: 40px;
    display: block;
    line-height: 30px;
    padding-bottom: 10px;
    font-style: italic;
    border-bottom: 2px #3D3F45 solid;
    border-left: 1px #3D3F45 solid;
    border-right: 1px #3D3F45 solid;
    display: inline-block;
    padding: 0px 30px 0px 30px;
    line-height: 35px;
    margin-top: 40px;
    background: #17181d;
    -webkit-border-radius: 4px;
    -moz-border-radius: 4px;
    border-radius: 4px;
}

.showmore {
    color: #494d5a;
    text-align: center;
    font-size: 17px;
    text-decoration: underline;
    display: block;
    font-weight: 700;
    line-height: 30px;
    display: inline-block;
    line-height: 28px;
    margin-top: 30px;
    background: #17181d;
    -webkit-border-radius: 4px;
    -moz-border-radius: 4px;
    border-radius: 4px;
    padding: 2px 30px 7px;
    -moz-box-shadow: 1px 1px 1px rgb(204, 204, 204, .5);
    -webkit-box-shadow: 1px 1px 1px rgb(204, 204, 204, .5);
    box-shadow: 1px 1px 1px rgb(204, 204, 204, .5);
}

/*Landing Page*/

.landingpage {
    width: 100%;
    padding: 0 0 45px !important;
    background: url(../images/landingbg-top.png)repeat top center;
    padding-top: 5px;
    display: block;
    margin-top: 94px;
    float: left;
}

.langingrelated {
    background: url(../images/rpropertry-bg.jpg)repeat top center !important;
    width: 100%;
    padding: 20px 0 50px 0;
    clear: both;
}

.landing-wrap {
    width: 999px;
    margin: 0 auto;
}

.breadcumb ul {
    margin: 0;
    padding: 0;
    list-style: none;
    margin-left: 16px;
    display: inline-block;

}

.breadcumb ul li {
    float: left;
    display: block;
    padding: 3px 5px;
    background: url("../images/breadcumb.png") no-repeat scroll right 6px top 12px rgba(0, 0, 0, 0);

}

.breadcumb ul li a {
    color: #9c9c9c;
    font-size: 12px;
    text-decoration: none;
    padding: 5px 0px;
    padding-right: 20px;
}

.breadcumb ul li .active {
    color: #d3d900 !important;
}

.breadcumb ul li:last-child {
    background-image: none !important;
}



.landing-wrap h3 {
    font-size: 25px;
    color: #d2d800;
    font-family: 'Conv_ProximaNova-Regular';
    display: block;
    line-height: 30px;
    font-weight: lighter;
    margin-left: 16px;
    margin-top: 25px;

}

.landing-slider {
    width: 640px;
    height: 475px;
    display: block;
    position: relative;
    margin-top: 4px;
    display: block;
    float: left;
    margin-left: 16px;
}


#bx-pager {
    margin: 0 auto;
    display: block;
    text-align: center;
    margin: 20px 10px;
    display: block;
}

.landing-slider .bx-wrapper .bx-controls-direction a {
    top: 9%;
    right: 10px;

}


.landing-slider .slider-ribbon {
    position: absolute;
    top: 5%;
    left: 0px;
}

#bx-pager a img {
    width: 112px;
    height: 77px;
    line-height: 0px;
    border: 2px #D2D800 solid;
    opacity: .4;
    outline: no;
}


#bx-pager a.active img {
    opacity: 1;
}

#bx-pager a {
    outline: none;
}


.landing-accordian {
    width: 310px;
