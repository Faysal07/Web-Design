    z-index: 99999999999;
}

.header-nav .social-icon ul {
    margin: 0;
    z-index: 99999999999;

}

.header-nav .social-icon ul li {
    background-color: #b60101;
    color: #fff;
    display: inline-block;
    float: right;
    width: 28px;
    height: 29px;
    line-height: 30px;
    list-style-type: none;
    margin: 23px 0 16px 20px;
    text-align: center;
    -webkit-transition: all 0.6s ease;
    -moz-transition: all 0.6s ease;
    -o-transition: all 0.6s ease;
    -ms-transition: all 0.6s ease;
    transition: all 0.6s ease;
}

.header-nav .social-icon ul li:hover {
    background-color: #fff;    
}

.header-nav .social-icon ul li:hover i {
    color: #28486d;
